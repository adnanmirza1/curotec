      <script>
         function success_story_scroll(){
         	var caseW = jQuery('.story_horizontal_scroll .elementor-widget-container .elementor-loop-container').outerWidth();
         	var bodyW = jQuery(window).width() / 2;
         	/*var bodyoff = jQuery(window).width() - jQuery('.story_horizontal_scroll_top .container').width();
         	var bodyoffG = bodyoff * 5;
         	jQuery('.story_horizontal_scroll .elementor-widget-container .elementor-loop-container').css('padding-left', bodyoffG);*/
         	jQuery('.story_horizontal_scroll').height((caseW + jQuery(window).height()) - jQuery(window).width());
         }
         
         jQuery(window).scroll(function(){ 
         		scrollDistance = 0;
         		var scrollDistance = jQuery(window).scrollTop(); 
         		if(jQuery('.story_horizontal_scroll').length > 0){ 
         			
         		var offset = jQuery(".story_horizontal_scroll .success_story_main_title").offset();
         	  var posY = offset.top - jQuery(window).scrollTop();
         		var posX = offset.left - jQuery(window).scrollLeft(); 
         		/*console.log('leftx:'+posX);*/ 
         			
         		
         		var bodyH = jQuery(window).height();
         		/*console.log("bodyH:"+bodyH);*/ 
         			
         		
         		var footer = jQuery('#next_section').offset().top - jQuery(window).height() + posX; 
         		var caseOff = jQuery('.story_horizontal_scroll').offset().top - posX; 
         		
         		if(bodyH<=700){
         			 jQuery('#story_horizontal_scroll .elementor-container.elementor-column-gap-no').css({ top: '-150px' });
         		}
         		else if(bodyH<=800){
         			 jQuery('#story_horizontal_scroll .elementor-container.elementor-column-gap-no').css({ top: '-100px' });
         		}
         		else if(bodyH<=900){
         			 jQuery('#story_horizontal_scroll .elementor-container.elementor-column-gap-no').css({ top: '-50px' });
         		}
         			
         		/*console.log('caseOff:'+ caseOff);
         		console.log('footer:'+ footer);
         		console.log('top:'+ (parseInt(jQuery(window).scrollTop())));*/
         			
         			if((parseInt(jQuery(window).scrollTop()))>2300){
         				jQuery('body').addClass('scroll_started');
         			}
         
         		if((parseInt(jQuery(window).scrollTop())) >= caseOff && (parseInt(jQuery(window).scrollTop())) <= footer){    
         			jQuery('body').addClass('scroll_started');
         			jQuery('.story_horizontal_scroll .elementor-widget-container .elementor-loop-container').css({'transform':'translate3d(-'+(scrollDistance - caseOff)+'px,0px, 0px)'}); 
         			jQuery('body.scroll_started .story_horizontal_scroll .elementor-widget-container .elementor-loop-container').css('padding-left', posX);
         		}
         
         		if((parseInt(jQuery(window).scrollTop())) < caseOff){ 
         			jQuery('body').removeClass('scroll_started');
         			jQuery('.story_horizontal_scroll .elementor-widget-container .elementor-loop-container').css({'padding-left':0,'transform':'translate3d(0px,0px, 0px)'});
         		} 
         	}
         });
         
         jQuery(window).load(function(){
         	success_story_scroll();
         	if((parseInt(jQuery(window).scrollTop()))>2300){
         		jQuery('body').addClass('scroll_started');
         	}
         	
         	var bodyH = jQuery(window).height();
         	/*console.log("bodyH:"+bodyH);*/ 
         });
         jQuery(window).resize(function(){
         	success_story_scroll();
         });
      </script>
